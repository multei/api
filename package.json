{
  "name": "@multei/api",
  "version": "0.1.0",
  "description": "Multei! API",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/multei/api.git"
  },
  "bugs": {
    "url": "https://github.com/multei/api/issues"
  },
  "homepage": "https://github.com/multei/api#readme",
  "private": false,
  "scripts": {
    "deploy:heroku": "git push heroku master",
    "develop:heroku": "nodemon --exec \"heroku local\" --signal SIGTERM",
    "heroku-postbuild": "knex migrate:latest",
    "install:clean": "rm -rf node_modules && npm install",
    "start": "node ./bin/www",
    "stryker": "stryker run",
    "test": "jest",
    "test:staged": "jest --bail --findRelatedTests --passWithNoTests $(git diff --name-only --diff-filter=ACM | grep \"js$\")"
  },
  "author": "Multei!",
  "contributors": [
    {
      "name": "Beto Maia",
      "email": "beto@multei.com.br",
      "url": "https://github.com/adalbertomaia"
    },
    {
      "name": "Jimmy Andrade",
      "email": "jimmy@multei.com.br",
      "url": "https://github.com/jimmyandrade"
    }
  ],
  "license": "MIT",
  "keywords": [
    "multei",
    "api",
    "node.js"
  ],
  "dependencies": {
    "@google-cloud/storage": "4.3.1",
    "cookie-parser": "1.4.4",
    "dotenv": "8.2.0",
    "debug": "4.1.1",
    "express": "4.17.1",
    "knex": "0.20.10",
    "morgan": "1.9.1",
    "multer": "1.4.2",
    "openalpr": "0.1.11",
    "pg": "7.18.2",
    "http-errors": "latest"
  },
  "devDependencies": {
    "@stryker-mutator/core": "2.5.0",
    "@stryker-mutator/html-reporter": "2.5.0",
    "@stryker-mutator/javascript-mutator": "2.5.0",
    "@stryker-mutator/jest-runner": "2.5.0",
    "@types/express": "4.17.2",
    "eslint": "6.8.0",
    "husky": "4.2.3",
    "jest": "25.1.0",
    "nodemon": "2.0.2",
    "supertest": "4.0.2"
  },
  "engines": {
    "node": ">=13.9.0",
    "npm": ">=6.13.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "sh bin/test-staged.sh",
      "pre-push": "npm test"
    }
  }
}
